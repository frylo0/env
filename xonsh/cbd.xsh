class CBD:
    @property
    def code(self):
        cd ~/Code

    @property
    def env(self):
        cd ~/Code/env

    @property
    def xonsh(self):
        cd ~/Code/env/xonsh

    @property
    def work(self):
        cd ~/Code/work/wb

    @property
    def pets(self):
        cd ~/Code/pets

    @property
    def downloads(self):
        cd ~/Downloads

    @property
    def localhost(self):
        cd /var/www

    @property
    def html(self):
        cd /var/www/html


cbd = CBD()
